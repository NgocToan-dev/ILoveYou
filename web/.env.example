# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# PWA Push Notifications
VITE_VAPID_PUBLIC_KEY=your_vapid_public_key

# Analytics and Monitoring
VITE_ANALYTICS_ENDPOINT=https://your-analytics-endpoint.com/api/metrics
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# App Configuration
VITE_APP_NAME=ILoveYou
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Track your love days, create notes and reminders for couples

# API Configuration
VITE_API_BASE_URL=https://api.iloveyou.com
VITE_CDN_BASE_URL=https://cdn.iloveyou.com

# Debug Settings (development only)
VITE_DEBUG_MODE=false
VITE_SHOW_PERFORMANCE_METRICS=false