{
  "expo": {
    "name": "ILoveYou",
    "slug": "iloveyou",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#FFB6C1"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.iloveyou.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFB6C1"
      },
      "package": "com.iloveyou.app"
    },
    "web": {
      "bundler": "metro"
    },
    "scheme": "iloveyou",
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#FFB6C1"
        }
      ],
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}
